{
    "name": "milan-miscevic/inert",
    "description": "Mini PHP framework with basic MVC and service container support",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "action",
        "action-container",
        "controller",
        "factory",
        "framework",
        "inert",
        "mini-php-framework",
        "mvc",
        "php-framework",
        "response",
        "service-container"
    ],
    "authors": [
        {
            "name": "Milan Miščević",
            "email": "milan.miscevic@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.7.0",
        "infection/infection": "^0.26.6",
        "phpstan/phpstan": "^1.4.9",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpunit/phpunit": "^9.5.18",
        "vimeo/psalm": "^4.22.0"
    },
    "autoload": {
        "psr-4": {
            "Mmm\\Inert\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mmm\\Inert\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {},
        "sort-packages": true
    }
}
