{
    "name": "milan-miscevic/inert",
    "description": "A mini PHP framework with basic MVC and service locator support",
    "keywords": ["framework", "mvc", "service locator"],
    "license": "MIT",
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.24",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Inert\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InertTest\\": "tests/"
        }
    }
}
